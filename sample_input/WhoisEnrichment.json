{
    "openapi": "3.0.2",
    "info": {
        "title": "WhoisEnrichment",
        "description": "**WhoisEnrichment** API helps you do retrieve awesome data. ðŸš€",
        "version": "0.0.1"
    },
    "paths": {
        "/": {
            "get": {
                "summary": "Root",
                "operationId": "root__get",
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    }
                }
            }
        },
        "/domainnames": {
            "get": {
                "tags": [
                    "whois"
                ],
                "summary": "Get Whois Name",
                "description": "Retrieve detailed whois information from domain name specified as parameter",
                "operationId": "get_whois_name_domainnames_get",
                "parameters": [
                    {
                        "required": true,
                        "schema": {
                            "title": "Domain Name",
                            "type": "string"
                        },
                        "name": "domain_name",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/ipv4": {
            "get": {
                "tags": [
                    "whois"
                ],
                "summary": "Get Whois Ip",
                "description": "Retrieve detailed whois information from IP address (v4 format) specified as parameter",
                "operationId": "get_whois_ip_ipv4_get",
                "parameters": [
                    {
                        "required": true,
                        "schema": {
                            "title": "Ip",
                            "type": "string"
                        },
                        "name": "ip",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "HTTPValidationError": {
                "title": "HTTPValidationError",
                "type": "object",
                "properties": {
                    "detail": {
                        "title": "Detail",
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/ValidationError"
                        }
                    }
                }
            },
            "ValidationError": {
                "title": "ValidationError",
                "required": [
                    "loc",
                    "msg",
                    "type"
                ],
                "type": "object",
                "properties": {
                    "loc": {
                        "title": "Location",
                        "type": "array",
                        "items": {
                            "anyOf": [
                                {
                                    "type": "string"
                                },
                                {
                                    "type": "integer"
                                }
                            ]
                        }
                    },
                    "msg": {
                        "title": "Message",
                        "type": "string"
                    },
                    "type": {
                        "title": "Error Type",
                        "type": "string"
                    }
                }
            }
        }
    },
    "tags": [
        {
            "name": "default",
            "description": "Home Page"
        },
        {
            "name": "whois",
            "description": "whois operations"
        }
    ]
}